<?xml version="1.0"?>
<fop version="1.0">
    <base>.</base>
    <source-resolution>72</source-resolution>
    <target-resolution>72</target-resolution>
    <default-page-settings height="29.7cm" width="21.0cm"/>
    <renderers>
        <renderer mime="application/pdf">
            <filterList>
                <value>flate</value>
            </filterList>
            <fonts>
                <directory>.</directory>
                <auto-detect/>
            </fonts>
        </renderer>
    </renderers>
</fop>